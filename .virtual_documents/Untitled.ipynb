import pandas as pd


df_titanic = pd.read_csv("Titanic-Dataset.csv")


df_titanic[["Name", "Sex", "Age", "Fare", "Survived"]]


df_titanic.query("Sex == 'female' and Fare > 30")


df_titanic.groupby("Pclass").agg(
    survival_rage = ("Survived", "mean"),
    average_fare = ("Fare", "mean"),
    average_age = ("Age", "mean")
)
